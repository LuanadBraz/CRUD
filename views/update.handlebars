<form name='formIns' action='controllerUpdate' method='post'>
    <b></b> <input type='hidden' name='id?' id='id?' value='{{id}}'><br>

    <div class="col-md-6 form-group">
    <label><b>Nome:</b> <input type='text' class="form-control" name='nome' id='nome' value='{{nome}}'></label></div>

    <div class="col-md-6 form-group">
    <label><b>Idade:</b> <input type='text' class="form-control" name='idade' id='idade' value='{{idade}}'></label></div>

    <div class="col-md-6 form-group">
    <label><b> Estado Civil:</b> <input type='text' class="form-control" name='civil' id='civil' value='{{civil}}'></label></div>

    <div class="col-md-6 form-group">
    <label><b>CPF:</b> <input type='text' class="form-control" name='cpf' id='cpf' value='{{cpf}}'></label></div>

    <div class="col-md-6 form-group">
    <label><b>Cidade:</b> <input type='text' class="form-control" name='cidade' id='cidade' value='{{cidade}}'></label></div>

    <div class="col-md-6 form-group">
    <label><b>Estado:</b> <input type='text' class="form-control" name='estado' id='estado' placeholder='SP' value='{{estado}}'></label></div>

    <div class="col-md-4"><strong>
    <b><button type='submit' name='btnSubmit' id='btnSubmit'>Atualizar</button></b></strong></div>
</form>